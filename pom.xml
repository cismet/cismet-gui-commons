
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>de.cismet.maven</groupId>
        <artifactId>cismetParent</artifactId>
        <version>1.0-SNAPSHOT</version>
    </parent>
    <groupId>de.cismet.commons</groupId>
    <artifactId>cismetGuiCommons</artifactId>
    <packaging>jar</packaging>
    <version>1.0-SNAPSHOT</version>
    <name>cismetGuiCommons</name>    
    <!--Platform specific sun tools.jar they are on every sun vm-->
    <profiles>
        <profile>
            <id>default-tools.jar</id>
            <activation>
                <property>
                    <name>java.vendor</name>
                    <value>Sun Microsystems Inc.</value>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>com.sun</groupId>
                    <artifactId>tools</artifactId>
                    <version>1.4.2</version>
                    <scope>system</scope>
                    <systemPath>${java.home}/../lib/tools.jar</systemPath>
                </dependency>
            </dependencies>
        </profile>
    </profiles>
    <dependencies>        
        <dependency>
            <groupId>net.eig.ws.util</groupId>
            <artifactId>WSSBean</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>de.cismet.commons</groupId>
            <artifactId>cismetCommons</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.swinglabs</groupId>
            <artifactId>swing-layout</artifactId>
            <version>1.0.3</version>
        </dependency>
        <dependency>
            <groupId>net.java.dev.fuse</groupId>
            <artifactId>fuse-core</artifactId>
            <version>0.4</version>
        </dependency>
        <dependency>
            <groupId>net.java.dev.fuse</groupId>
            <artifactId>fuse-swing</artifactId>
            <version>0.4</version>
        </dependency>
        <dependency>
            <groupId>net.java.dev.fuse</groupId>
            <artifactId>fuse-swt</artifactId>
            <version>0.4</version>
        </dependency>
        <dependency>
            <groupId>net.dev.java</groupId>
            <artifactId>pdf-renderer</artifactId>
            <version>unknown</version>
        </dependency>
        <dependency>
            <groupId>groovy</groupId>
            <artifactId>groovy-all</artifactId>
            <version>1.1-rc-1</version>
        </dependency>
        <!--Swingx transitiv dependency from cismetCommons-->
        <!--Commons transitiv dependency from WSSBean-->
    </dependencies>
</project>


